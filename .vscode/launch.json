{
  "version": "0.2.0",
  "configurations": [
    {
      // You can begin Electron with npm run dev in the terminal, and then
      // attach to the main process with the debug view.
      "name": "Attach",
      "type": "node",
      "request": "attach",
      "port": 9362,
      "protocol": "legacy"
    },
    {
      // You should disable (comment out) the Chrome dev tools in main.js
      // if you wish to debug the renderer process in VSCode.
      "name": "Attach Electron Renderer",
      "type": "chrome",
      "request": "attach",
      "webRoot": "${workspaceRoot}",
      "port": 9361
    }
  ]
}